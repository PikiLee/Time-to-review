{
	"name": "time-to-review",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite --port 4173 --host",
		"build": "run-p type-check build-only",
		"preview": "vite preview",
		"test:unit": "vitest --environment jsdom --root src/",
		"test:e2e": "cypress run --e2e",
		"test:e2e:open": "cypress open --e2e",
		"coverage": "vitest run --coverage --environment jsdom",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
	},
	"dependencies": {
		"@unocss/reset": "^0.48.0",
		"@vueuse/core": "^9.9.0",
		"@vueuse/shared": "^9.12.0",
		"@zodios/core": "^10.7.7",
		"axios": "^1.2.6",
		"dayjs": "^1.11.7",
		"element-plus": "^2.2.28",
		"lodash-es": "^4.17.21",
		"mark.js": "^8.11.1",
		"pinia": "^2.0.29",
		"shared": "workspace:*",
		"sortablejs": "^1.15.0",
		"vue": "^3.2.45",
		"vue-i18n": "9",
		"vue-router": "^4.1.6",
		"zod": "^3.20.6"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.0",
		"@intlify/vite-plugin-vue-i18n": "^7.0.0",
		"@rushstack/eslint-patch": "^1.1.4",
		"@testing-library/cypress": "^9.0.0",
		"@testing-library/dom": "^9.0.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/user-event": "^14.4.3",
		"@types/jsdom": "^20.0.1",
		"@types/lodash-es": "^4.17.6",
		"@types/mark.js": "^8.11.8",
		"@types/node": "^18.11.12",
		"@types/sortablejs": "^1.15.0",
		"@unocss/preset-attributify": "^0.48.0",
		"@unocss/preset-icons": "^0.48.0",
		"@unocss/preset-mini": "^0.48.0",
		"@unocss/preset-typography": "^0.48.0",
		"@unocss/preset-uno": "^0.48.0",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vitest/coverage-c8": "^0.28.4",
		"@vue/eslint-config-prettier": "^7.0.0",
		"@vue/eslint-config-typescript": "^11.0.0",
		"@vue/test-utils": "^2.2.6",
		"@vue/tsconfig": "^0.1.3",
		"cypress": "^12.4.1",
		"eslint": "^8.22.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-vue": "^9.3.0",
		"jsdom": "^20.0.3",
		"local-cypress": "^1.2.6",
		"msw": "^1.0.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.3",
		"sass": "^1.57.1",
		"start-server-and-test": "^1.15.2",
		"typescript": "~4.7.4",
